{% if slots %}
    <ul>
    {% for slot in slots %}
    <li>{{slot.product.productName }} - {{slot.product.price}} - {{slot.quantity}} - {{slot.slotNr}}</li>
    {% endfor %}
    </ul>
	
	<form action="" method="post">
		{% csrf_token %}
		<table>
		{{ form.as_table }}
		</table>
		<input type="submit" value="Submit">
	</form>
{% endif %}

{% if status %}
	{{status}}
{% endif %}
